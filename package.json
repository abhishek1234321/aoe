{
  "name": "aoe",
  "version": "0.1.0",
  "description": "Amazon Order Extractor browser extension",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "npm run dev:all",
    "dev:main": "VITE_MINIFY=false VITE_SOURCEMAP=true vite build --mode main --watch",
    "dev:content": "VITE_MINIFY=false VITE_SOURCEMAP=true vite build --mode content --watch",
    "dev:all": "concurrently \"npm:dev:main\" \"npm:dev:content\"",
    "clean": "rm -rf dist",
    "build": "npm run build:main && npm run build:content",
    "build:main": "vite build --mode main",
    "build:content": "vite build --mode content",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "test:ci": "vitest --run",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishek1234321/aoe.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhishek1234321/aoe/issues"
  },
  "homepage": "https://github.com/abhishek1234321/aoe#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "date-fns": "3.6.0",
    "papaparse": "5.4.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.269",
    "@types/firefox-webext-browser": "143.0.0",
    "@types/node": "22.7.0",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "1.6.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.1.0",
    "jsdom": "24.1.0",
    "prettier": "3.3.3",
    "typescript": "5.9.3",
    "vite": "5.3.4",
    "vitest": "1.6.0"
  }
}
