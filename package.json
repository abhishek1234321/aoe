{
  "name": "aoe",
  "version": "0.1.1",
  "description": "Amazon Order Extractor browser extension",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "npm run dev:all",
    "dev:main": "VITE_MINIFY=false VITE_SOURCEMAP=true vite build --mode main --watch",
    "dev:content": "VITE_MINIFY=false VITE_SOURCEMAP=true vite build --mode content --watch",
    "dev:all": "concurrently \"npm:dev:main\" \"npm:dev:content\"",
    "clean": "rm -rf dist",
    "build": "npm run build:main && npm run build:content",
    "build:e2e": "VITE_E2E=true npm run build",
    "build:main": "vite build --mode main",
    "build:content": "vite build --mode content",
    "check": "npm run lint && npm run typecheck && npm run test:ci && npm run format:check && npm run fixtures:check",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest --passWithNoTests",
    "test:ci": "vitest --run",
    "fixtures:check": "vitest --run tests/fixtures.test.ts",
    "e2e": "npm run build:e2e && node scripts/prepare-e2e.mjs && playwright test",
    "e2e:ci": "node scripts/prepare-e2e.mjs && playwright test",
    "e2e:install": "playwright install chromium",
    "screenshots": "npm run build:e2e && node scripts/prepare-e2e.mjs && node scripts/capture-store-screenshots.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security:audit": "npm audit --audit-level=high"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishek1234321/aoe.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhishek1234321/aoe/issues"
  },
  "homepage": "https://github.com/abhishek1234321/aoe#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "date-fns": "4.1.0",
    "papaparse": "5.5.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "webextension-polyfill": "0.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/chrome": "0.1.36",
    "@types/firefox-webext-browser": "143.0.0",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "9.2.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "jsdom": "27.4.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "vitest": {
    "exclude": [
      "tests/e2e/**"
    ]
  }
}
